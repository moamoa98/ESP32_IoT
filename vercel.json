{
    "builds": [{
        "env": {
      "PYTHON_VERSION": "3.10"
    },
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        }
    ]
}